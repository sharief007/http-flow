{
  "name": "http-flow",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "python main.py",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "build": "vite build",
    "build:backend": "python -m py_compile backend/main.py",
    "lint": "eslint .",
    "lint:backend": "python -m flake8 backend/",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:backend": "python -m pytest backend/tests/",
    "test:all": "npm run test:backend && npm run test",
    "clean": "rimraf dist node_modules/.vite",
    "install:backend": "pip install -r requirements.txt",
    "setup": "npm install && npm run install:backend"
  },
  "dependencies": {
    "flatbuffers": "^25.2.10",
    "immer": "^10.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^3.4.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^13.24.0",
    "jsdom": "^24.1.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.0",
    "vite": "^5.4.0",
    "vitest": "^1.6.0"
  }
}
